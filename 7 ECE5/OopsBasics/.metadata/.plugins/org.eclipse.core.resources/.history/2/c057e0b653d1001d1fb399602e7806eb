package asg1;
	
		import java.util.Scanner;
		public class A {

		 public static void main(String[] args) {
		  Scanner cs=new Scanner(System.in);
		  int num,i;
		     System.out.println("Enter a number:");
		     num=cs.nextInt();
		     int count=0;
		     for(i=2;i<=num/2;i++)
		     {
		        if(num%i==0)
		         count++;
		     }
		    if(count==0)
		      System.out.println("It is Prime ");
		    else
		         System.out.println("It is not Prime ");
		  cs.close();
		 }
		}
