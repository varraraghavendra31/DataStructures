package in.raghava.programspractice.small;

public class Pattern24 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(" ");
			}
			for(int j=5;j<=1;j--) {
				System.out.print(j);	
			} 
			System.out.println();
		}
	}

}
