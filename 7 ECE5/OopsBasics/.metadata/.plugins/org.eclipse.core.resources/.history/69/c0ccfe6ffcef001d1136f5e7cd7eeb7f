package in.raghava.programspractice.small;

public class Pattern24 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=5;j++) {
				if(i==1||i==5) {
					System.out.println("*");
					
				}
				System.out.println();
//				else if(j==5-i){
//					System.out.println("*");
//				
//				}
				else {
					System.out.println();
				}
				
					
			} 
		}
	}

}
