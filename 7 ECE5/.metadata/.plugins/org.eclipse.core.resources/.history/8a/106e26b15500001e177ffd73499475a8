package TwoDarrays;

public class DiagonalSumArray {
public static void main(String[] args) {
	int[][]a= {{1,2,3,1},{3,1,1,4},{3,1,1,9},{1,5,7,1}};
	int sum=0;
	for(int i=0;i<a.length;i++) {
		for(int j=0;j<a[i].length;j++) {
			if(i==j) {
				sum+=a[i][j];
			}
			if(i+j==a.length-1) {
				sum+=a[i][j];
			}
		}
	}
	System.out.println("entered  elements diagonal sum "+sum);
}
}
