package Programs;

public class PrimeRecursion {
public static void main(String[] args) {
	boolean b=prime(2,2);
	System.out.println(b);
}
static boolean prime(int n,int i) {
	    if(i<=n/2) {
		if(n%i==0)
			return false;
	    }
	    if(n<1)return false;
	    else   
	        prime(n, i+1);
	     return true;
	    
	
}

}
