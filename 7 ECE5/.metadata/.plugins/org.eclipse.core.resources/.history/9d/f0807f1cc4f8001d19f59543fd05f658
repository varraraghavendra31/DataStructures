package Programs;

public class PrimeRecursion {
public static void main(String[] args) {
	String b=prime(5,2);
	System.out.println(b);
}
static String prime(int n,int i) {
	if(n==2)return "its a prime";
	else 
		if(i<n/2) {
			if(n%i==0) return "its not a prime";
			prime(n,i+1);
		}
		else
			return "it a prime";
	return "its a prime";
			
	    
	     
	    
	
}

}
