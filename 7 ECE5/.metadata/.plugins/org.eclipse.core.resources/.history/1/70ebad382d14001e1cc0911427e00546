package StringPrograms;
import java.util.Scanner;
public class SecondSmallNumInArray {
public static void main(String[] args) {
	
	int[]a= {7,7,8,12,0,-3,5};
	int first=a[0];
	int second=a[0];
	int count=1;
	for(int i=1;i<a.length;i++) {
		if(count==1) {
			if(first<a[i]) {
				first=a[i];
				count++;
			}
		}
		if(a[i]<first) {
			second=first;
			first=a[i];	
		}
		else if(a[i]<second) {
				second=a[i];
			}
		else if(a[i]==first) {
			continue;
		}
	}
	if(first!=second) {
		System.out.println(first);
	}
	System.out.println(second);
}
}
