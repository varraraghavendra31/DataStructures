package Programs;

public class PrimeRecursion {
public static void main(String[] args) {
	String b=prime(3,2);
	System.out.println(b);
}
static String prime(int n,int i) {
	        if(i <= n)
	        {
	            if(n%i!=0) prime(n, ++i);
	            else
	            	return "its a prime"; 
	            
	        }
	        return "its not a prime";
	    }
	
}

