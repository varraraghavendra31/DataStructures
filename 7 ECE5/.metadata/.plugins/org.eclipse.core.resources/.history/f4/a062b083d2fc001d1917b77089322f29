package com.ravveseh.strings;

public class RverseTheWordEndToStart {
public static void main(String[] args) {
	String s="java is easy";
	int k=s.length();
	String s2="";
	for(int i=k;i>=0;i--) {
		
		if(s.charAt(i)==' ')
		{
			for(int j=i+1;j>=k;j++) {
				s2+=s.charAt(j);
			}
		}
	}
	System.out.println(s2);
}
}
