package com.patterns;

import java.util.Scanner;
public class InterviewPattern {
	public static void main(String[] args) {
		System.out.println("enter a num");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				if(i==3) {
					for(int k=1;k<=3;k++) {
						System.out.print(++i+" ");
					}
					i=3;
					break;
				}
				System.out.print("*");
			}
			System.out.println();
			}
			
			
		}
	}
	

